<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="main" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.gui.controller.GuiController">
   <children>
      <VBox alignment="TOP_CENTER" maxWidth="350.0" minWidth="250.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox fx:id="pdfListBox" maxWidth="350.0" minWidth="210.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <HBox fx:id="imageBox" alignment="CENTER" style="-fx-background-color: #fff;" HBox.hgrow="ALWAYS">
         <children>
            <ImageView fx:id="imageView" fitHeight="202.0" fitWidth="200.0" pickOnBounds="true" smooth="false" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</HBox>
